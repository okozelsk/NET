  <!-- Synapse configuration -->
  <xs:complexType name="SynapseType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="SynapseSTType" name="spikingTarget" minOccurs="0" maxOccurs="1"/>
      <xs:element type="SynapseATType" name="analogTarget" minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>


  <!-- Predictors -->
  <!-- Activation predictor -->
  <xs:complexType name="PredictorActivationType"/>

  <!-- Activation power predictor -->
  <xs:complexType name="PredictorActivationPowerType">
    <xs:attribute type="UDoubleGT0Type" name="exponent" use="optional" default="2">
      <xs:annotation>
        <xs:documentation xml:lang="en">The exponent. Default value is 2.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:boolean" name="keepSign" use="optional" default="false">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies whether to keep the original sign of the activation value. Default value is true.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Activation stat figure predictor -->
  <xs:complexType name="PredictorActivationStatFigureType">
    <xs:attribute type="UIntGE2LE1024NAOptionType" name="window" use="optional" default="NA">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies the data window size. Default value is NA.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="StatFigure" name="figure" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The statistical figure.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Activation rescaled range predictor -->
  <xs:complexType name="PredictorActivationRescaledRangeType">
    <xs:attribute type="UIntGE2LE1024Type" name="window" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies the data window size.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Activation linearly weighted average predictor -->
  <xs:complexType name="PredictorActivationLinWAvgType">
    <xs:attribute type="UIntGE2LE1024NAOptionType" name="window" use="optional" default="NA">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies the data window size. Default value is NA.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Activation differences stat figure predictor -->
  <xs:complexType name="PredictorActivationDiffStatFigureType">
    <xs:attribute type="UIntGE2LE1024NAOptionType" name="window" use="optional" default="NA">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies the data window size. Default value is NA.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="StatFigure" name="figure" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The statistical figure.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Activation differences rescaled range predictor -->
  <xs:complexType name="PredictorActivationDiffRescaledRangeType">
    <xs:attribute type="UIntGE2LE1024Type" name="window" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies the data window size.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Activation differences linearly weighted average predictor -->
  <xs:complexType name="PredictorActivationDiffLinWAvgType">
    <xs:attribute type="UIntGE2LE1024NAOptionType" name="window" use="optional" default="NA">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies the data window size. Default value is NA.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>


  <!-- Firing trace predictor -->
  <xs:complexType name="PredictorFiringTraceType">
    <xs:attribute type="UIntGE2LE1024NAOptionType" name="window" use="optional" default="NA">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies the data window size. Default value is NA.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleLE1Type" name="fading" use="optional" default="0.005">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies the strength of trace fading. Default value is 0.005.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>


  <!-- Predictors configuration -->
  <xs:complexType name="PredictorsType">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element type="PredictorActivationType" name="activation" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">The result of the activation function.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element type="PredictorActivationPowerType" name="activationPower" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">The powered absolute value of the result of the activation function.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element type="PredictorActivationStatFigureType" name="activationStatFigure" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">The statistical feature computed from the activation function results.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element type="PredictorActivationRescaledRangeType" name="activationRescaledRange" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">The rescaled range computed from the activation function results.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element type="PredictorActivationLinWAvgType" name="activationLinWAvg" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">The linearly weighted average computed from the activation function results.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element type="PredictorActivationDiffStatFigureType" name="activationDiffStatFigure" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">The statistical figure computed from the differences of the activation function results (A[T] - A[T-1]).</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element type="PredictorActivationDiffRescaledRangeType" name="activationDiffRescaledRange" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">The rescaled range computed from the differences of the activation function results (A[T] - A[T-1]).</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element type="PredictorActivationDiffLinWAvgType" name="activationDiffLinWAvg" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">The linearly weighted average computed from the differences of the activation function results (A[T] - A[T-1]).</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element type="PredictorFiringTraceType" name="firingTrace" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">The traced neuron's firing.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
  </xs:complexType>


  <!-- Pool interconnection schemas -->
  <!-- Empty schema -->
  <xs:complexType name="PoolInterconnectionEmptySchemaType">
  </xs:complexType>

  <!-- Random schema -->
  <xs:complexType name="PoolInterconnectionRandomSchemaType">
    <xs:attribute type="UDoubleGT0LE1Type" name="density" use="optional" default="0.1">
      <xs:annotation>
        <xs:documentation xml:lang="en">The density of interconnected neurons. Default value is 0.1.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleGT0NAOptionType" name="avgDistance" use="optional" default="NA">
      <xs:annotation>
        <xs:documentation xml:lang="en">The average distance of interconnected neurons (NA means the random distance). Default value is NA.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:boolean" name="allowSelfConnection" use="optional" default="true">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies whether to allow neurons to be self connected. Default value is true.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:boolean" name="constantNumOfConnections" use="optional" default="false">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies whether to keep the constant number of synapses. Default value is false.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:boolean" name="replaceExistingConnections" use="optional" default="true">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies whether the connections of this schema will replace the existing connections. Default value is true.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UIntGT0Type" name="repetitions" use="optional" default="1">
      <xs:annotation>
        <xs:documentation xml:lang="en">The number of applications of this schema. Default value is 1.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Chain schema -->
  <xs:complexType name="PoolInterconnectionChainSchemaType">
    <xs:attribute type="UDoubleGT0LE1Type" name="ratio" use="optional" default="1">
      <xs:annotation>
        <xs:documentation xml:lang="en">The ratio of involved neurons. Default value is 1.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:boolean" name="circle" use="optional" default="true">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies whether the chain will be closed to a circle. Default value is true.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:boolean" name="replaceExistingConnections" use="optional" default="true">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies whether the connections of this schema will replace the existing connections. Default value is true.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UIntGT0Type" name="repetitions" use="optional" default="1">
      <xs:annotation>
        <xs:documentation xml:lang="en">The number of applications of this schema. Default value is 1.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Double twisted toroid schema -->
  <xs:complexType name="PoolInterconnectionDoubleTwistedToroidSchemaType">
    <xs:attribute type="UDoubleGT0LE1Type" name="ratio" use="optional" default="1">
      <xs:annotation>
        <xs:documentation xml:lang="en">The ratio of involved neurons. Default value is 1.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:boolean" name="lDiagonalSelf" use="optional" default="false">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies whether the left diagonal neurons to be self connected. Default value is false.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:boolean" name="rDiagonalSelf" use="optional" default="false">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies whether the right diagonal neurons to be self connected. Default value is false.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:boolean" name="replaceExistingConnections" use="optional" default="true">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies whether the connections of this schema will replace the existing connections. Default value is true.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UIntGT0Type" name="repetitions" use="optional" default="1">
      <xs:annotation>
        <xs:documentation xml:lang="en">The number of applications of this schema. Default value is 1.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Pool interconnection configuration -->
  <xs:complexType name="PoolInterconnectionType">
    <xs:choice minOccurs="1" maxOccurs="1">
      <xs:sequence minOccurs="1" maxOccurs="1">
        <xs:element type="PoolInterconnectionEmptySchemaType" name="emptySchema" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
      <xs:sequence minOccurs="1" maxOccurs="1">
        <xs:choice minOccurs="1" maxOccurs="unbounded">
          <xs:element type="PoolInterconnectionRandomSchemaType" name="randomSchema" minOccurs="1" maxOccurs="1"/>
          <xs:element type="PoolInterconnectionChainSchemaType" name="chainSchema" minOccurs="1" maxOccurs="1"/>
          <xs:element type="PoolInterconnectionDoubleTwistedToroidSchemaType" name="doubleTwistedToroidSchema" minOccurs="1" maxOccurs="1"/>
        </xs:choice>
      </xs:sequence>
    </xs:choice>
  </xs:complexType>

  <!-- Homogenous excitability configuration -->
  <xs:complexType name="HomogenousExcitabilityType">
    <xs:attribute type="UDoubleGT0Type" name="excitatoryStrength" use="optional" default="0.75">
      <xs:annotation>
        <xs:documentation xml:lang="en">The total excitatory strength. Default value is 0.75.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleLE1Type" name="inputRatio" use="optional" default="0.75">
      <xs:annotation>
        <xs:documentation xml:lang="en">The input strength ratio. Default value is 0.75.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleLE1Type" name="inhibitoryRatio" use="optional" default="0.25">
      <xs:annotation>
        <xs:documentation xml:lang="en">The inhibitory strength ratio. Default value is 0.25.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>


  <!-- Spiking neuron group configuration -->
  <xs:complexType name="PoolSpikingNeuronGroupType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:choice minOccurs="1" maxOccurs="1">
        <xs:element type="ActivationSimpleIFType" name="activationSimpleIF" minOccurs="1" maxOccurs="1"/>
        <xs:element type="ActivationLeakyIFType" name="activationLeakyIF" minOccurs="1" maxOccurs="1"/>
        <xs:element type="ActivationExpIFType" name="activationExpIF" minOccurs="1" maxOccurs="1"/>
        <xs:element type="ActivationAdExpIFType" name="activationAdExpIF" minOccurs="1" maxOccurs="1"/>
        <xs:element type="ActivationIzhikevichIFType" name="activationIzhikevichIF" minOccurs="1" maxOccurs="1"/>
        <xs:element type="ActivationAutoIzhikevichIFType" name="activationAutoIzhikevichIF" minOccurs="1" maxOccurs="1"/>
      </xs:choice>
      <xs:element type="HomogenousExcitabilityType" name="homogenousExcitability" minOccurs="0" maxOccurs="1"/>
      <xs:element type="RandomValueType" name="bias" minOccurs="0" maxOccurs="1"/>
      <xs:element type="PredictorsType" name="predictors" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="name" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The name of the neuron group.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleType" name="relShare" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies how big relative portion of pool's neurons is formed by this group of the neurons.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Analog neuron group -->
  <!-- Retainment configuration -->
  <xs:complexType name="RetainmentType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="URandomValueType" name="strength" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
    <xs:attribute type="UDoubleLE1Type" name="density" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies the ratio of the neurons having the Retainment property.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Analog neuron group configuration -->
  <xs:complexType name="PoolAnalogNeuronGroupType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:choice minOccurs="1" maxOccurs="1">
        <xs:element type="ActivationSQNLType" name="activationSQNL" minOccurs="1" maxOccurs="1"/>
        <xs:element type="ActivationElliotType" name="activationElliot" minOccurs="1" maxOccurs="1"/>
        <xs:element type="ActivationGaussianType" name="activationGaussian" minOccurs="1" maxOccurs="1"/>
        <xs:element type="ActivationISRUType" name="activationISRU" minOccurs="1" maxOccurs="1"/>
        <xs:element type="ActivationSigmoidType" name="activationSigmoid" minOccurs="1" maxOccurs="1"/>
        <xs:element type="ActivationSincType" name="activationSinc" minOccurs="1" maxOccurs="1"/>
        <xs:element type="ActivationSinusoidType" name="activationSinusoid" minOccurs="1" maxOccurs="1"/>
        <xs:element type="ActivationTanHType" name="activationTanH" minOccurs="1" maxOccurs="1"/>
      </xs:choice>
      <xs:element type="RandomValueType" name="bias" minOccurs="0" maxOccurs="1"/>
      <xs:element type="RetainmentType" name="retainment" minOccurs="0" maxOccurs="1"/>
      <xs:element type="PredictorsType" name="predictors" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="name" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The name of the neuron group.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleType" name="relShare" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies how big relative portion of pool's neurons is formed by this group of the neurons.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleLE1Type" name="firingThreshold" use="optional" default="0.00125">
      <xs:annotation>
        <xs:documentation xml:lang="en">The firing threshold value. Every time the current normalized activation is higher than the normalized past reference activation by at least this threshold, it is evaluated as a firing event. Default value is 0.00125.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UIntGT0Type" name="thresholdMaxRefDeepness" use="optional" default="1">
      <xs:annotation>
        <xs:documentation xml:lang="en">Maximum age of the past activation for the evaluation of the firing event. Default value is 1.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Neuron groups choice-->
  <xs:complexType name="PoolNeuronGroupsType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:choice minOccurs="1" maxOccurs="unbounded">
        <xs:element type="PoolAnalogNeuronGroupType" name="analogGroup" minOccurs="1" maxOccurs="1"/>
        <xs:element type="PoolSpikingNeuronGroupType" name="spikingGroup" minOccurs="1" maxOccurs="1"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>

  <!-- Pool configuration -->
  <xs:complexType name="PoolType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="ProportionsType" name="proportions" minOccurs="1" maxOccurs="1"/>
      <xs:element type="CoordinatesType" name="coordinates" minOccurs="0" maxOccurs="1"/>
      <xs:element type="PoolNeuronGroupsType" name="neuronGroups" minOccurs="1" maxOccurs="1">
        <xs:unique name="unique_neuron_group_name">
          <xs:selector xpath="./*"/>
          <xs:field xpath="@name"/>
        </xs:unique>
      </xs:element>
      <xs:element type="PoolInterconnectionType" name="interconnection" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="name" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The name of the pool.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>


  <!-- Reservoir structure -->
  <!-- Pools -->
  <xs:complexType name="ResStructPoolsType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="PoolType" name="pool" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <!-- Inter-pool connection -->
  <xs:complexType name="ResStructInterPoolConnectionType">
    <xs:attribute type="xs:string" name="targetPool" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The name of the target pool.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleLE1Type" name="targetConnDensity" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">Determines how many neurons in the target pool get connected source pool neurons.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:string" name="sourcePool" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The name of the source pool.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleLE1Type" name="sourceConnDensity" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">Determines how many neurons from the source pool to be connected to one neuron from target pool.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:boolean" name="constantNumOfConnections" use="optional" default="false">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies whether to keep constant number of connections from source neurons to target neuron. Default value is false.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Inter-pool connections -->
  <xs:complexType name="ResStructInterPoolConnectionsType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="ResStructInterPoolConnectionType" name="interPoolConnection" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <!-- Reservoir structure configuration -->
  <xs:complexType name="ResStructType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="ResStructPoolsType" name="pools" minOccurs="1" maxOccurs="1">
        <xs:unique name="resstruct_unique_pool_name">
          <xs:selector xpath="./pool"/>
          <xs:field xpath="@name"/>
        </xs:unique>
      </xs:element>
      <xs:element type="ResStructInterPoolConnectionsType" name="interPoolConnections" minOccurs="0" maxOccurs="1">
        <xs:unique name="resstruct_unique_pool2pool">
          <xs:selector xpath="./interPoolConnection"/>
          <xs:field xpath="@srcPool"/>
          <xs:field xpath="@targetPool"/>
        </xs:unique>
      </xs:element>
    </xs:sequence>
    <xs:attribute type="xs:string" name="name" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The name of the reservoir structure configuration.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Neural preprocessor -->
  <!-- Input -->
  <!-- Continuous input feeding configuration -->
  <xs:complexType name="NPInpFeedingContinuousType">
    <xs:attribute type="UIntGT0AutoOptionType" name="bootCycles" use="optional" default="Auto">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies the number of the boot cycles. Default value is Auto.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Patterned input feeding -->
  <!-- Resampling -->
  <xs:complexType name="NPResamplingType">
    <xs:attribute type="UDoubleLT1Type" name="signalBeginThreshold" use="optional" default="0">
      <xs:annotation>
        <xs:documentation xml:lang="en">The threshold of the signal begin detection. Default value is 0 (maximum sensitivity).</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleLT1Type" name="signalEndThreshold" use="optional" default="0">
      <xs:annotation>
        <xs:documentation xml:lang="en">Threshold of signal end detection. Default value is 0 (maximum sensitivity).</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:boolean" name="uniformTimeScale" use="optional" default="true">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies whether all the variables in the input pattern should have the same signal begin/end. Default value is true.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UIntGT1AutoOptionType" name="targetTimePoints" use="optional" default="Auto">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies whether the input pattern variable's data will be upsampled and/or downsampled to have specified fixed length (GT 0). Default value is Auto (keeps original time points).</xs:documentation>
