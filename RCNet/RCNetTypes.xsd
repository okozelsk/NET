        <xs:element type="ActivationSincCfgType" name="activationSinc" minOccurs="1" maxOccurs="1"/>
        <xs:element type="ActivationSinusoidCfgType" name="activationSinusoid" minOccurs="1" maxOccurs="1"/>
        <xs:element type="ActivationTanHCfgType" name="activationTanH" minOccurs="1" maxOccurs="1"/>
      </xs:choice>
      <xs:element type="RandomValueType" name="bias" minOccurs="0" maxOccurs="1"/>
      <xs:element type="PoolAnalogNeuronsFiringThresholdCfgType" name="firingThreshold" minOccurs="0" maxOccurs="1"/>
      <xs:element type="PoolAnalogNeuronsRetanmentCfgType" name="retainment" minOccurs="0" maxOccurs="1"/>
      <xs:element type="PredictorsCfgType" name="predictors" minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="name" use="required" />
    <xs:attribute type="NeuronRoleType" name="role" use="required"/>
    <xs:attribute type="UDoubleType" name="relShare" use="required"/>
    <xs:attribute type="NeuronSignalingRestrictionType" name="signalingRestriction" use="optional" default="NoRestriction">
      <xs:annotation>
        <xs:documentation xml:lang="en">Default value is NoRestriction</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleType" name="readoutDensity" use="optional" default="1">
      <xs:annotation>
        <xs:documentation xml:lang="en">Default value is 1</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Pool - neuron groups - configuration -->
  <xs:complexType name="PoolNeuronGroupsCfgType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:choice minOccurs="1" maxOccurs="unbounded">
        <xs:element type="PoolAnalogNeuronGroupCfgType" name="analogGroup" minOccurs="1" maxOccurs="1"/>
        <xs:element type="PoolSpikingNeuronGroupCfgType" name="spikingGroup" minOccurs="1" maxOccurs="1"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>

  <!-- Pool configuration -->
  <xs:complexType name="PoolCfgType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="PoolNeuronGroupsCfgType" name="neuronGroups" minOccurs="1" maxOccurs="1">
        <xs:unique name="unique_neuron_group_name">
          <xs:selector xpath="./*"/>
          <xs:field xpath="@name"/>
        </xs:unique>
      </xs:element>
      <xs:element type="PredictorsCfgType" name="predictors" minOccurs="0" maxOccurs="1"/>
      <xs:element type="PoolInterconnectionCfgType" name="interconnection" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="name" use="required" />
    <xs:attribute type="xs:int" name="x" use="optional" default="0">
      <xs:annotation>
        <xs:documentation xml:lang="en">Default value is 0</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:int" name="y" use="optional" default="0">
      <xs:annotation>
        <xs:documentation xml:lang="en">Default value is 0</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:int" name="z" use="optional" default="0">
      <xs:annotation>
        <xs:documentation xml:lang="en">Default value is 0</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:unsignedInt" name="dimX" use="required" />
    <xs:attribute type="xs:unsignedInt" name="dimY" use="required" />
    <xs:attribute type="xs:unsignedInt" name="dimZ" use="required" />
  </xs:complexType>

  <!-- ________________________________________RESERVOIR TYPES_________________________________________________ -->

  <!-- Reservoir - input entry point - configuration -->
  <xs:complexType name="ReservoirInputEntryPointType">
    <xs:attribute type="xs:int" name="x" use="required" />
    <xs:attribute type="xs:int" name="y" use="required" />
    <xs:attribute type="xs:int" name="z" use="required" />
  </xs:complexType>

  <!-- Reservoir - pools container - configuration -->
  <xs:complexType name="ReservoirPoolCfgContainerType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="PoolCfgType" name="pool" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <!-- Reservoir - pool to pool connection - configuration -->
  <xs:complexType name="ReservoirPool2PoolConnCfgType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="InternalSynapseCfgType" name="synapse" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="srcPool" use="required"/>
    <xs:attribute type="UDoubleLE1Type" name="srcConnDensity" use="required"/>
    <xs:attribute type="xs:string" name="targetPool" use="required"/>
    <xs:attribute type="UDoubleLE1Type" name="targetConnDensity" use="required"/>
    <xs:attribute type="UDoubleType" name="relShareEE" use="optional" default="0.3">
      <xs:annotation>
        <xs:documentation xml:lang="en">Default value is 0.3</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleType" name="relShareEI" use="optional" default="0.2">
      <xs:annotation>
        <xs:documentation xml:lang="en">Default value is 0.2</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleType" name="relShareIE" use="optional" default="0.4">
      <xs:annotation>
        <xs:documentation xml:lang="en">Default value is 0.4</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleType" name="relShareII" use="optional" default="0.1">
      <xs:annotation>
        <xs:documentation xml:lang="en">Default value is 0.1</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:boolean" name="constantNumOfConnections" use="optional" default="false">
      <xs:annotation>
        <xs:documentation xml:lang="en">Default value is false</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Reservoir - pools interconnection container - configuration -->
  <xs:complexType name="ReservoirPool2PoolConnCfgContainerType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="ReservoirPool2PoolConnCfgType" name="pool2PoolConn" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <!-- Reservoir - spectral radius - value - configuration -->
  <xs:complexType name="ReservoirSpectralRadiusValueCfgType">
    <xs:attribute type="UDoubleGT0NAOptionType" name="value" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">Use NA to disable spectral radius application.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  
  <!-- Reservoir - spectral radius - configuration -->
  <xs:complexType name="ReservoirSpectralRadiusCfgType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="ReservoirSpectralRadiusValueCfgType" name="analogScope" minOccurs="1" maxOccurs="1"/>
      <xs:element type="ReservoirSpectralRadiusValueCfgType" name="spikingScope" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <!-- Reservoir configuration -->
  <xs:complexType name="ReservoirCfgType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="ReservoirInputEntryPointType" name="inputEntryPoint" minOccurs="0" maxOccurs="1"/>
      <xs:element type="ReservoirPoolCfgContainerType" name="pools" minOccurs="1" maxOccurs="1">
        <xs:unique name="unique_pool_name">
          <xs:selector xpath="./pool"/>
          <xs:field xpath="@name"/>
        </xs:unique>
      </xs:element>
      <xs:element type="ReservoirPool2PoolConnCfgContainerType" name="pool2PoolConns" minOccurs="0" maxOccurs="1">
        <xs:unique name="unique_pool2pool_pools">
          <xs:selector xpath="./pool2PoolConn"/>
          <xs:field xpath="@srcPool"/>
          <xs:field xpath="@targetPool"/>
        </xs:unique>
      </xs:element>
      <xs:element type="ReservoirSpectralRadiusCfgType" name="spectralRadius" minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="name" use="required" />
  </xs:complexType>

  <!-- ________________________________________NEURAL PREPROCESSOR TYPES_________________________________________________ -->
  <!-- NeuralPreprocessor - continuous input feeding -configuration -->
  <xs:complexType name="NPInpFeedingContinuousCfgType">
    <xs:attribute type="UIntGT0AutoOptionType" name="bootCycles" use="optional" default="Auto">
      <xs:annotation>
        <xs:documentation xml:lang="en">Default value is Auto</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:boolean" name="routeToReadout" use="optional" default="false">
      <xs:annotation>
        <xs:documentation xml:lang="en">Default value is false</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- NeuralPreprocessor - patterned input feeding -configuration -->
  <xs:complexType name="NPInpFeedingPatternedCfgType">
  </xs:complexType>

  <!-- NeuralPreprocessor - external input field -configuration -->
  <xs:complexType name="NPExternalInpFieldCfgType">
    <xs:choice minOccurs="1" maxOccurs="1">
      <xs:element type="BinFeatureFilterCfgType" name="binFeature" minOccurs="0" maxOccurs="1"/>
      <xs:element type="EnumFeatureFilterCfgType" name="enumFeature" minOccurs="0" maxOccurs="1"/>
      <xs:element type="RealFeatureFilterCfgType" name="realFeature" minOccurs="0" maxOccurs="1"/>
    </xs:choice>
    <xs:attribute type="xs:string" name="name" use="required"/>
    <xs:attribute type="xs:boolean" name="allowRoutingToReadout" use="optional" default="true">
      <xs:annotation>
        <xs:documentation xml:lang="en">Default value is true</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- NeuralPreprocessor - internal input field -configuration -->
  <xs:complexType name="NPInternalInpFieldCfgType">
    <xs:choice minOccurs="1" maxOccurs="1">
      <xs:element type="PulseGeneratorCfgType" name="pulse" minOccurs="1" maxOccurs="1"/>
      <xs:element type="RandomValueType" name="random" minOccurs="1" maxOccurs="1"/>
      <xs:element type="SinusoidalGeneratorCfgType" name="sinusoidal" minOccurs="1" maxOccurs="1"/>
      <xs:element type="MackeyGlassGeneratorCfgType" name="mackeyGlass" minOccurs="1" maxOccurs="1"/>
    </xs:choice>
    <xs:attribute type="xs:string" name="name" use="required"/>
    <xs:attribute type="xs:boolean" name="allowRoutingToReadout" use="optional" default="false">
      <xs:annotation>
        <xs:documentation xml:lang="en">Default value is false</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>


  <!-- NeuralPreprocessor - internal input fields -configuration -->
  <xs:complexType name="NPInternalInpFieldsCfgType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="NPInternalInpFieldCfgType" name="field" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>


  <!-- NeuralPreprocessor - external input fields -configuration -->
  <xs:complexType name="NPExternalInpFieldsCfgType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="NPExternalInpFieldCfgType" name="field" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <!-- NeuralPreprocessor - input -configuration -->
  <xs:complexType name="NPInputCfgType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:choice minOccurs="1" maxOccurs="1">
        <xs:element type="NPInpFeedingContinuousCfgType" name="feedingContinuous" minOccurs="1" maxOccurs="1"/>
        <xs:element type="NPInpFeedingPatternedCfgType" name="feedingPatterned" minOccurs="1" maxOccurs="1"/>
      </xs:choice>
      <xs:element type="NPExternalInpFieldsCfgType" name="external" minOccurs="1" maxOccurs="1"/>
      <xs:element type="NPInternalInpFieldsCfgType" name="internal" minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <!-- NeuralPreprocessor - reservoirs configuration - container -->
  <xs:complexType name="NPResCfgContainerType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="ReservoirCfgType" name="reservoirCfg" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <!-- NeuralPreprocessor - reservoir instance - input field connection configuration -->
  <xs:complexType name="NPResInstanceInputConnectionCfgType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="InputSynapseCfgType" name="synapse" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="fieldName" use="required"/>
    <xs:attribute type="xs:string" name="poolName" use="required"/>
    <xs:attribute type="UDoubleGT0Type" name="density" use="required"/>
  </xs:complexType>

  <!-- NeuralPreprocessor - reservoir instance - input field connection - configuration collection -->
  <xs:complexType name="NPResInstanceInputConnectionsCfgType">
    <xs:sequence minOccurs="1" maxOccurs="unbounded">
      <xs:element type="NPResInstanceInputConnectionCfgType" name="inputConnection" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <!-- NeuralPreprocessor - reservoir instance - configuration -->
  <xs:complexType name="NPResInstanceCfgType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="NPResInstanceInputConnectionsCfgType" name="inputConnections" minOccurs="1" maxOccurs="1">
        <xs:unique name="unique_input_field_assignment">
          <xs:selector xpath="./inputFieldAssignment"/>
          <xs:field xpath="@inputFieldName"/>
          <xs:field xpath="@poolName"/>
        </xs:unique>
      </xs:element>
      <xs:element type="PredictorsCfgType" name="predictors" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="name" use="required" />
    <xs:attribute type="xs:string" name="cfg" use="required" />
  </xs:complexType>

  <!-- NeuralPreprocessor - reservoir instances - container -->
  <xs:complexType name="NPResInstanceContainerType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="NPResInstanceCfgType" name="reservoirInstance" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <!-- NeuralPreprocessor configuration -->
  <xs:complexType name="NPCfgType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="NPInputCfgType" name="input" minOccurs="1" maxOccurs="1">
        <xs:unique name="unique_input_field_name">
          <xs:selector xpath=".//field"/>
          <xs:field xpath="@name"/>
        </xs:unique>
      </xs:element>
      <xs:element type="NPResCfgContainerType" name="reservoirCfgContainer" minOccurs="1" maxOccurs="1">
        <xs:unique name="unique_reservoir_configuration_name">
          <xs:selector xpath="./reservoirCfg"/>
          <xs:field xpath="@name"/>
        </xs:unique>
      </xs:element>
      <xs:element type="NPResInstanceContainerType" name="reservoirInstanceContainer" minOccurs="1" maxOccurs="1">
        <xs:unique name="unique_reservoir_instance_name">
          <xs:selector xpath="./reservoirInstance"/>
          <xs:field xpath="@name"/>
        </xs:unique>
      </xs:element>
    </xs:sequence>
  </xs:complexType>


  <!-- ________________________________________STATE MACHINE TYPES_________________________________________________ -->
  <!-- StateMachine - Mapper allowed pool predictors - configuration -->
  <xs:complexType name="SMAllowedPredictorsPoolCfgType">
    <xs:attribute type="xs:string" name="reservoirInstanceName" use="required"/>
    <xs:attribute type="xs:string" name="poolName" use="required"/>
  </xs:complexType>

  <!-- StateMachine - Mapper allowed pool predictors for specific readout unit - configuration -->
  <xs:complexType name="SMReadoutUnitAllowedPredictorsCfgType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="SMAllowedPredictorsPoolCfgType" name="allowed" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="readoutUnitName" use="required"/>
  </xs:complexType>

  <!-- StateMachine - Mapper - configuration -->
  <xs:complexType name="SMPredictorsToReadoutUnitsMapType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="SMReadoutUnitAllowedPredictorsCfgType" name="map" minOccurs="1" maxOccurs="unbounded">
        <xs:unique name="unique_res_instance_pool_name">
          <xs:selector xpath="./allowed"/>
          <xs:field xpath="@reservoirInstanceName"/>
          <xs:field xpath="@poolName"/>
        </xs:unique>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!-- StateMachine complete configuration -->
  <xs:complexType name="SMCfgType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="NPCfgType" name="neuralPreprocessor" minOccurs="1" maxOccurs="1">
        <xs:key name="input_field_name">
          <xs:selector xpath="./input/*/field"/>
          <xs:field xpath="@name"/>
        </xs:key>
        <xs:keyref name="existing_input_field" refer="input_field_name">
          <xs:selector xpath=".//inputConnection" />
          <xs:field xpath="@fieldName" />
        </xs:keyref>
      </xs:element>
      <xs:element type="ROutLayerCfgType" name="readoutLayer" minOccurs="1" maxOccurs="1">
        <xs:unique name="unique_output_field_name">
          <xs:selector xpath="./readoutUnit"/>
          <xs:field xpath="@name"/>
        </xs:unique>
      </xs:element>
      <xs:element type="SMPredictorsToReadoutUnitsMapType" name="mapper" minOccurs="0" maxOccurs="1">
        <xs:unique name="unique_readout_unit_name">
          <xs:selector xpath="./map"/>
          <xs:field xpath="@readoutUnitName"/>
        </xs:unique>
      </xs:element>
    </xs:sequence>
    <xs:attribute type="xs:int" name="randomizerSeek" use="optional" default="0">
      <xs:annotation>
        <xs:documentation xml:lang="en">Default value is 0</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

</xs:schema>

