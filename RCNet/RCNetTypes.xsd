  <xs:complexType name="SynapseATType">
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element type="SynapseATInputType" name="input" minOccurs="0" maxOccurs="1"/>
      <xs:element type="SynapseATIndifferentType" name="indifferent" minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
    <xs:attribute type="UDoubleGT0NAOptionType" name="spectralRadius" use="optional" default="0.9999">
      <xs:annotation>
        <xs:documentation xml:lang="en">The spectral radius (use NA to disable spectral radius application). Default value is 0.9999.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
