<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="RCNetTypes"
           attributeFormDefault="unqualified"
           elementFormDefault="unqualified"
           xmlns="RCNetTypes"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           >
  <!-- Defines the RCNet types. -->

  <!-- Simple types -->
  <!-- Enumerations -->
  <!-- The type of random distribution -->
  <xs:simpleType name="RandomDistributionType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Uniform">
        <xs:annotation>
          <xs:documentation xml:lang="en">The uniform distribution.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Gaussian">
        <xs:annotation>
          <xs:documentation xml:lang="en">The gaussian distribution.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Exponential">
        <xs:annotation>
          <xs:documentation xml:lang="en">The exponential distribution.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Gamma">
        <xs:annotation>
          <xs:documentation xml:lang="en">The gamma distribution.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <!-- The pulse timing mode -->
  <xs:simpleType name="PulseGeneratorTimingMode">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Constant">
        <xs:annotation>
          <xs:documentation xml:lang="en">The period of the pulses is constant.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Uniform">
        <xs:annotation>
          <xs:documentation xml:lang="en">The period of the pulses follows the Uniform distribution.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Gaussian">
        <xs:annotation>
          <xs:documentation xml:lang="en">The period of the pulses follows the Gaussian distribution.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Poisson">
        <xs:annotation>
          <xs:documentation xml:lang="en">The period of the pulses follows the Poisson (Exponential) distribution.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <!-- The way of bidirectional processing of input pattern -->
  <xs:simpleType name="NPBidirProcessing">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Continuous">
        <xs:annotation>
          <xs:documentation xml:lang="en">Enabled bi-directional processing without reservoir reset when the direction to be turned.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="WithReset">
        <xs:annotation>
          <xs:documentation xml:lang="en">Enabled bi-directional processing with reservoir reset when the direction to be turned.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Forbidden">
        <xs:annotation>
          <xs:documentation xml:lang="en">The bi-directional processing is forbidden.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <!-- The statistical figure -->
  <xs:simpleType name="StatFigure">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Sum">
        <xs:annotation>
          <xs:documentation xml:lang="en">The sum of all samples.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="NegSum">
        <xs:annotation>
          <xs:documentation xml:lang="en">The sum of negative samples.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="PosSum">
        <xs:annotation>
          <xs:documentation xml:lang="en">The sum of positive samples.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SumOfSquares">
        <xs:annotation>
          <xs:documentation xml:lang="en">The sum of squared samples.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Min">
        <xs:annotation>
          <xs:documentation xml:lang="en">The min sample.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Max">
        <xs:annotation>
          <xs:documentation xml:lang="en">The max sample.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Mid">
        <xs:annotation>
          <xs:documentation xml:lang="en">The center value between the Min and Max.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Span">
        <xs:annotation>
          <xs:documentation xml:lang="en">The span of the Min and Max (Max - Min).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ArithAvg">
        <xs:annotation>
          <xs:documentation xml:lang="en">The arithmetic average.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="MeanSquare">
        <xs:annotation>
          <xs:documentation xml:lang="en">The mean of the squared samples.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="RootMeanSquare">
        <xs:annotation>
          <xs:documentation xml:lang="en">The root of the mean of the squared samples.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Variance">
        <xs:annotation>
          <xs:documentation xml:lang="en">The variance of the samples.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="StdDev">
        <xs:annotation>
          <xs:documentation xml:lang="en">The standard deviation of the samples.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SpanDev">
        <xs:annotation>
          <xs:documentation xml:lang="en">The span multiplied by the standard deviation of the samples.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <!-- Schema of variables organization in an input pattern -->
  <xs:simpleType name="PatternVariablesSchema">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Groupped">
        <xs:annotation>
          <xs:documentation xml:lang="en">[v1(t1),v2(t1),v1(t2),v2(t2),v1(t3),v2(t3)] where "v" means variable and "t" means time point.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Sequential">
        <xs:annotation>
          <xs:documentation xml:lang="en">[v1(t1),v1(t2),v1(t3),v2(t1),v2(t2),v2(t3)] where "v" means variable and "t" means time point.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <!-- The synaptic delay method -->
  <xs:simpleType name="SynapticDelayMethod">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Random">
        <xs:annotation>
          <xs:documentation xml:lang="en">The synaptic delay is decided randomly.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Distance">
        <xs:annotation>
          <xs:documentation xml:lang="en">The synaptic delay depends on an Euclidean distance.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <!-- The synapse's role -->
  <xs:simpleType name="SynapseRole">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Input">
        <xs:annotation>
          <xs:documentation xml:lang="en">An input synapse.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Excitatory">
        <xs:annotation>
          <xs:documentation xml:lang="en">An excitatory synapse.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Inhibitory">
        <xs:annotation>
          <xs:documentation xml:lang="en">An inhibitory synapse.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Indifferent">
        <xs:annotation>
          <xs:documentation xml:lang="en">An indifferent synapse.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <!-- The type of an activation function -->
  <xs:simpleType name="ActivationType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Spiking">
        <xs:annotation>
          <xs:documentation xml:lang="en">A spiking activation function. Attempts to simulate the behavior of a biological neuron that accumulates (integrates) input stimulation on its membrane potential and when the critical threshold is exceeded, fires a short pulse (spike), resets membrane and the cycle starts from the beginning. In other words, the function implements one of the so-called Integrate and Fire neuron models.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Analog">
        <xs:annotation>
          <xs:documentation xml:lang="en">An analog activation function. It has no similarity to behavior of the biological neuron. It is always stateless, which means that the output value (signal) does not depend on the previous inputs but only on current input at the time T and particular transformation equation (usually non-linear).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <!-- An identifier of the predictor -->
  <xs:simpleType name="PredictorID">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Activation">
        <xs:annotation>
          <xs:documentation xml:lang="en">The result of the activation function.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ActivationPower">
        <xs:annotation>
          <xs:documentation xml:lang="en">The powered absolute value of the result of the activation function.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ActivationStatFigure">
        <xs:annotation>
          <xs:documentation xml:lang="en">The statistical figure computed from the activation function results.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ActivationRescaledRange">
        <xs:annotation>
          <xs:documentation xml:lang="en">The rescaled range computed from the activation function results.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ActivationLinWAvg">
        <xs:annotation>
          <xs:documentation xml:lang="en">The linearly weighted average computed from the activation function results.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ActivationDiffStatFigure">
        <xs:annotation>
          <xs:documentation xml:lang="en">The statistical figure computed from the differences of the activation function results (A[T] - A[T-1]).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ActivationDiffRescaledRange">
        <xs:annotation>
          <xs:documentation xml:lang="en">The rescaled range computed from the differences of the activation function results (A[T] - A[T-1]).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ActivationDiffLinWAvg">
        <xs:annotation>
          <xs:documentation xml:lang="en">The linearly weighted average computed from the differences of the activation function results (A[T] - A[T-1]).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="FiringTrace">
        <xs:annotation>
          <xs:documentation xml:lang="en">The traced neuron's firing.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <!-- The ODE solving method -->
  <xs:simpleType name="ODENumSolverMethod">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Euler">
        <xs:annotation>
          <xs:documentation xml:lang="en">The Euler 1st order method.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="RK4">
        <xs:annotation>
          <xs:documentation xml:lang="en">The Runge-Kutta 4th order method.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <!-- The way of input spikes coding -->
  <xs:simpleType name="InputSpikesCoding">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Horizontal">
        <xs:annotation>
          <xs:documentation xml:lang="en">The horizontal coding.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Vertical">
        <xs:annotation>
          <xs:documentation xml:lang="en">The vertical coding.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Forbidden">
        <xs:annotation>
          <xs:documentation xml:lang="en">The coding of input spikes is not allowed.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <!-- Auto option-->
  <xs:simpleType name="AutoOptionType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Auto"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- NA option-->
  <xs:simpleType name="NAOptionType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="NA"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- Numeric types -->
  <!-- Nonzero unsigned integer -->
  <xs:simpleType name="UIntGT0Type">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="1" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned integer GE 1 and LE 128-->
  <xs:simpleType name="UIntGE1LE128Type">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="128" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned integer GE 2 and LE 128-->
  <xs:simpleType name="UIntGE2LE128Type">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="2" />
      <xs:maxInclusive value="128" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned integer GE 1 and LE 64-->
  <xs:simpleType name="UIntGE1LE64Type">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="64" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned integer GE 1 and LE 32-->
  <xs:simpleType name="UIntGE1LE32Type">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="32" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned integer GE 2 and LE 64-->
  <xs:simpleType name="UIntGE2LE64Type">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="2" />
      <xs:maxInclusive value="64" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned integer GE 2 and LE 32-->
  <xs:simpleType name="UIntGE2LE32Type">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="2" />
      <xs:maxInclusive value="32" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned integer greater than 1-->
  <xs:simpleType name="UIntGT1Type">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="2" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned integer greater than 2-->
  <xs:simpleType name="UIntGT2Type">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="3" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned integer greater than 3-->
  <xs:simpleType name="UIntGT3Type">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="4" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned integer greater equal to 2 and less equal to 18-->
  <xs:simpleType name="UIntGE2LE18Type">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="2" />
      <xs:maxInclusive value="18" />
    </xs:restriction>
  </xs:simpleType>

  <!-- UIntGT0Type with Auto option-->
  <xs:simpleType name="UIntGT0AutoOptionType">
    <xs:union memberTypes="UIntGT0Type AutoOptionType"/>
  </xs:simpleType>

  <!-- UIntGT1Type with Auto option-->
  <xs:simpleType name="UIntGT1AutoOptionType">
    <xs:union memberTypes="UIntGT1Type AutoOptionType"/>
  </xs:simpleType>

  <!-- Unsigned integer GE 1 and LE 1024-->
  <xs:simpleType name="UIntGE1LE1024Type">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="8192" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned integer GE 2 and LE 1024-->
  <xs:simpleType name="UIntGE2LE1024Type">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="2" />
      <xs:maxInclusive value="8192" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned integer GE 2 and LE 1024 with NA option-->
  <xs:simpleType name="UIntGE2LE1024NAOptionType">
    <xs:union memberTypes="UIntGE2LE1024Type NAOptionType"/>
  </xs:simpleType>

  <!-- Double with NA option-->
  <xs:simpleType name="DoubleNAOptionType">
    <xs:union memberTypes="xs:double NAOptionType"/>
  </xs:simpleType>

  <!-- Double LT 0 -->
  <xs:simpleType name="DoubleLT0Type">
    <xs:restriction base="xs:double">
      <xs:maxExclusive value="0" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned double -->
  <xs:simpleType name="UDoubleType">
    <xs:restriction base="xs:double">
      <xs:minInclusive value="0" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Double not EQ 0 -->
  <xs:simpleType name="DoubleNotEq0Type">
    <xs:union memberTypes="DoubleLT0Type UDoubleGT0Type"/>
  </xs:simpleType>

  <!-- Unsigned double GT 0-->
  <xs:simpleType name="UDoubleGT0Type">
    <xs:restriction base="xs:double">
      <xs:minExclusive value="0" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned double GT 0 LT 1-->
  <xs:simpleType name="UDoubleGT0LT1Type">
    <xs:restriction base="xs:double">
      <xs:minExclusive value="0" />
      <xs:maxExclusive value="1" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned double GT 0 LE 1-->
  <xs:simpleType name="UDoubleGT0LE1Type">
    <xs:restriction base="xs:double">
      <xs:minExclusive value="0" />
      <xs:maxInclusive value="1" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned double GT 0 LE 0.5-->
  <xs:simpleType name="UDoubleGT0LE05Type">
    <xs:restriction base="xs:double">
      <xs:minExclusive value="0" />
      <xs:maxInclusive value="0.5" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned double GT 0 LE 32-->
  <xs:simpleType name="UDoubleGT0LE32Type">
    <xs:restriction base="xs:double">
      <xs:minExclusive value="0" />
      <xs:maxInclusive value="32" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned double GE 0 LT 1-->
  <xs:simpleType name="UDoubleLT1Type">
    <xs:restriction base="xs:double">
      <xs:minInclusive value="0" />
      <xs:maxExclusive value="1" />
    </xs:restriction>
  </xs:simpleType>

  <!-- UDoubleGT0Type with NA option-->
  <xs:simpleType name="UDoubleGT0NAOptionType">
    <xs:union memberTypes="UDoubleGT0Type NAOptionType"/>
  </xs:simpleType>

  <!-- Unsigned double GE 1-->
  <xs:simpleType name="UDoubleGE1Type">
    <xs:restriction base="xs:double">
      <xs:minInclusive value="1" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned double greater than 1 -->
  <xs:simpleType name="UDoubleGT1Type">
    <xs:restriction base="xs:double">
      <xs:minExclusive value="1" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Unsigned double less than or equal 1 -->
  <xs:simpleType name="UDoubleLE1Type">
    <xs:restriction base="xs:double">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="1" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Complex types -->
  <!-- 3D space -->
  <!-- Coordinates -->
  <xs:complexType name="CoordinatesType">
    <xs:attribute type="xs:int" name="x" use="optional" default="0">
      <xs:annotation>
        <xs:documentation xml:lang="en">The X coordinate. Default value is 0.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:int" name="y" use="optional" default="0">
      <xs:annotation>
        <xs:documentation xml:lang="en">The Y coordinate. Default value is 0.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:int" name="z" use="optional" default="0">
      <xs:annotation>
        <xs:documentation xml:lang="en">The Z coordinate. Default value is 0.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Proportions -->
  <xs:complexType name="ProportionsType">
    <xs:attribute type="UIntGT0Type" name="dimX" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The X dimension.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UIntGT0Type" name="dimY" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The Y dimension.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UIntGT0Type" name="dimZ" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The Z dimension.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>


  <!-- Random -->
  <!-- Uniform distribution -->
  <xs:complexType name="UniformDistrType">
  </xs:complexType>

  <!-- Gaussian distribution -->
  <xs:complexType name="GaussianDistrType">
    <xs:attribute type="xs:double" name="mean" use="optional" default="0">
      <xs:annotation>
        <xs:documentation xml:lang="en">The mean. Default value is 0.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleGT0Type" name="stdDev" use="optional" default="1">
      <xs:annotation>
        <xs:documentation xml:lang="en">The standard deviation. Default value is 1.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Exponential distribution -->
  <xs:complexType name="ExponentialDistrType">
    <xs:attribute type="DoubleNotEq0Type" name="mean" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The mean.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Gamma distribution -->
  <xs:complexType name="GammaDistrType">
    <xs:attribute type="UDoubleGT0Type" name="alpha" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The alpha (shape parameter).</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleGT0Type" name="beta" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The beta (rate parameter).</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Random value -->
  <xs:complexType name="RandomValueType">
    <xs:choice minOccurs="0" maxOccurs="1">
      <xs:element type="UniformDistrType" name="uniformDistr" minOccurs="1" maxOccurs="1"/>
      <xs:element type="GaussianDistrType" name="gaussianDistr" minOccurs="1" maxOccurs="1"/>
      <xs:element type="ExponentialDistrType" name="exponentialDistr" minOccurs="1" maxOccurs="1"/>
      <xs:element type="GammaDistrType" name="gammaDistr" minOccurs="1" maxOccurs="1"/>
    </xs:choice>
    <xs:attribute type="xs:double" name="min" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The min value (inclusive).</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:double" name="max" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">The max value (exclusive).</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:boolean" name="randomSign" use="optional" default="false">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies whether to randomize the value sign. Default value is false.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Unsigned gaussian distribution -->
  <xs:complexType name="UGaussianDistrType">
    <xs:attribute type="UDoubleGT0Type" name="mean" use="optional" default="0.5">
      <xs:annotation>
        <xs:documentation xml:lang="en">The mean. Default value is 0.5.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="UDoubleGT0Type" name="stdDev" use="optional" default="1">
      <xs:annotation>
        <xs:documentation xml:lang="en">The standard deviation. Default value is 1.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

